{
  "id": "example_config",
  "train_params": {
    "sequences_length": 1000,
    "batch_size": 64,
    "learning_rate": 1e-3,
    "num_epochs": 10
  },
  "test_params": {
    "macroseq_length": 60,
    "min_feature_threshold": 1e-6,
    "max_feature_threshold": 1e-4,
    "number_feature_thresholds_to_try": 10
  },
  "data_params": {
    "sensor_number": 0,
    "train_data" : {
      "first_date": "18/01/1998",
      "last_date": "25/01/1998"
    },
    "validation_train_data": {
      "first_date": "18/01/1998",
      "last_date": "25/01/1998"
    },
    "validation_test_data": {
      "first_date": "18/01/1998",
      "last_date": "25/01/1998"
    },
    "test_data": {
      "first_date": "18/01/1998",
      "last_date": "25/01/1998"
    }
  },

  "network_params": {
    "type_model": "keras_model",
    "optimizer": "adam",
    "loss_function": "mean_squared_error",
    "topology": [
      {
        "layer": "dense_keras_layer",
        "neurons": 256,
        "input_shape": [1000],
        "activation": "tanh"
      },
      {
        "layer": "dense_keras_layer",
        "neurons": 128,
        "activation": "tanh"
      },
      {
        "layer": "dense_keras_layer",
        "neurons": 256,
        "activation": "tanh"
      },
      {
        "layer": "dense_keras_layer",
        "neurons": 1000,
        "activation": "sigmoid"
      }
    ]
  },

  "preprocessing_steps": [
    {
      "type": "sequence_splitter",
      "params": {
        "sequences_length": 1000
      }
    },
    {
      "type": "sequence_normalizer",
      "params": {
        "range": [-1, 1]
      }
    }
  ]
}